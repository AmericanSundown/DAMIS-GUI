{% block body %}
    <div class="col-lg-12">
        <form method="POST">
            <div class="form-group">
                {{ form_label(form.resultType) }}
                {{ form_widget(form.resultType) }}
                {{ form_errors(form.resultType) }}
            </div>
            <div class="form-group">
                {{ form_label(form.zValue) }}
                {{ form_widget(form.zValue) }}
                {{ form_errors(form.zValue) }}
            </div>
            {{ form_widget(form._token) }}
        </form>
    </div>
{% endblock %}