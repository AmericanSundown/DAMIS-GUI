{% block body %}
    <div class="col-lg-12">
        <form method="POST">
            <input type="hidden" name="params" id="params" value="{{ response }}" />
            <div class="form-group">
                {{ form_label(form.retFilteredData) }}
                {{ form_widget(form.retFilteredData) }}
                {{ form_errors(form.retFilteredData) }}
            </div>
            <div class="form-group">
                {{ form_label(form.zValue) }}
                {{ form_widget(form.zValue) }}
                {{ form_errors(form.zValue) }}
            </div>
            {{ form_widget(form._token) }}
        </form>
    </div>
{% endblock %}